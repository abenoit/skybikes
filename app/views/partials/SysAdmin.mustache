<div class="Admin">
  <h4>Stations status</h4>
  <div class="stations-cards">
  {{#stations}}
  <div class="demo-card-square mdl-card mdl-shadow--2dp">
    <div class="mdl-card__title mdl-card--expand">
      <h2 class="mdl-card__title-text">{{location}}</h2>
    </div>
    <div class="mdl-card__supporting-text">
      <span>Bikes Availables : {{bikesAvailable}}</span>
    </div>
    <div class="mdl-card__supporting-text">
      <span>Free Slots : {{nbFreeSlot}}</span>
    </div>
  </div>
  {{/stations}}
  </div>

  <h4>Members information</h4>
  <table class="mdl-data-table mdl-js-data-table mdl-shadow--2dp">
    <thead>
      <tr>
        <th>Email</th>
        <th>Name</th>
        <th>Phone</th>
        <th>Is banned</th>
        <th>Is currently renting a bike</th>
      </tr>
    </thead>
    <tbody>
      {{#members}}
          <tr>
          <td>{{email}}</td>
          <td>{{firstname}} {{lastname}}</td>
          <td>{{phone}}</td>
          <td>{{#banned}}
              Yes
              {{/banned}}
              {{^banned}}
              No
              {{/banned}}
          </td>
          <td>{{#rentalInfo}}
              Yes
              {{/rentalInfo}}
              {{^rentalInfo}}
              No
              {{/rentalInfo}}
          </td>
          </tr>
      {{/members}}
    </tbody>
  </table>
</div>